{
    "sma_modbus_meter": {
		"module": "ModbusMeter",
		"config_implementation": {
			"main": {
				"modbus_ip_address": "192.168.178.67",
				"modbus_port": 502,
				"power_unit_id": 3,
				"power_in_register": 30865,
				"power_in_length": 2,
				"power_out_register": 30867,
				"power_out_length": 2,
				"energy_unit_id": 3,
				"energy_in_register": 30581,
				"energy_in_length": 2,
				"energy_out_register": 30583,
				"energy_out_length": 2,
                "update_interval": 1000
			}
		}
	},
    "emgr_logger": {
        "module": "JsEmgrLogger",
        "config_implementation": {
            "main": {
                "file_path_prefix": "emgr_data"
            }
        },
        "connections": {
            "emgr": [
                {
                    "module_id": "energy_manager",
                    "implementation_id": "main"
                }
            ],
            "pm_grid": [
                {
                    "module_id": "sma_modbus_meter",
                    "implementation_id": "main"
                }
            ],
            "pm_ev": [
                {
                    "module_id": "yeti_driver",
                    "implementation_id": "powermeter"
                }
            ]
        }
    },
    "energy_manager": {
        "module": "JsEnergyManager",
        "config_implementation": {
            "main": {
                "pid_setpoint": 500,
                "pid_p_weight": 0.4,
                "pid_i_weight": 0.1,
                "pid_d_weight": 0.2,
                "pid_output_interval": 5000,
                "pid_i_limit":-1,
                "pid_min_output": 1380,
                "pid_max_output": 7360
            }
        },
        "connections": {
            "chargingdriver": [
                {
                    "module_id": "yeti_driver",
                    "implementation_id": "yeti"
                }
            ],
            "gridpowermeter": [
                {
                    "module_id": "sma_modbus_meter",
                    "implementation_id": "main"
                }
            ],
            "chargingdriverpowermeter": [
                {
                    "module_id": "yeti_driver",
                    "implementation_id": "powermeter"
                }
            ]
        }
    },
    "iso15118_charger": {
        "module": "JsRiseV2G",
        "config_implementation": {
            "main": {
                "mqtt_base_path": "everest_external/iso15118/java"
            }
        }
    },
    "yeti_driver": {
        "module": "YetiDriver",
        "config_module": {
            "serial_port": "/dev/serial0",
            "baud_rate": 115200
        },
        "config_implementation": {
            "yeti": {
                "country_code": "DE",
                "has_ventilation": true,
                "three_phases": true
            },
            "rcd": {
                "rcd_enabled": true
            }
        }
    }
}
